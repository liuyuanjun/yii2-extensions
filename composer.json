{
    "name": "liuyuanjun/yii2-extensions",
    "description": "Some extensions for Yii2.",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "ext-libxml": "*",
        "yiisoft/yii2": "~2.0",
        "liuyuanjun/yii2-softdelete": "^1.0",
        "yiisoft/yii2-redis": "^2.0",
        "symfony/var-dumper": "^5.3",
        "guzzlehttp/guzzle": "^7.3",
        "aliyuncs/oss-sdk-php": "^2.4",
        "phpoffice/phpspreadsheet": "^1.18",
        "alibabacloud/sdk": "^1.8",
        "fakerphp/faker": "^1.15"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "LiuYuanjun",
            "email": "6879391@qq.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "liuyuanjun\\yii2\\extensions\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
